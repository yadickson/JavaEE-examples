##################################################################
# For MariaDB
##################################################################
# console
#          mysql -u<user> -p<password>
#
# list users
#          SELECT USER FROM mysql.user;
#
# show databases
#          show databases;
#
# run external sql file
#          \. <fullpath sql file>
#
# connecte to database
#          use <database>;
#

##################################################################
# For PostgreSQL
##################################################################
# login posgres user
#          sudo su
#          su postgres
#
# console
#          psql
#
# list users
#          \du
#
# show databases
#          \l
#
# run external sql file
#          \i <fullpath sql file>
#
# connecte to database
#          \c <database>
#
# create database and user
#          \i <>/admin.sql
#
# exit normal user and create tables
#
# $ psql -h localhost -d health_insurance -U insurance
#
#          \i <>/structure.sql

##################################################################
# For Oracle
##################################################################
# Install
# http://www.javavault.it/2015/01/27/install-oracle-11g-xe-on-debian/


sqlplus / as sysdba
@/home/debian/SpringMVC/sql/oracle/admin.sql

sqlplus insurance/insurance
@/home/debian/SpringMVC/sql/oracle/structure.sql

Enjoy
